dev:
  propel:
    param:
      classname:  DebugPDO
all:
  propel:
    class:        sfPropelDatabase
    param:
      classname:  PropelPDO
      dsn:        mysql:dbname=$DB_NAME;host=$DB_HOST
      username:   $DB_USERNAME
      password:   $DB_PASSWORD
      encoding:   utf8
      persistent: true
      pooling:    true
